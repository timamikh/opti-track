{
  "name": "opti-track",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "keystone": "cd keystone-cms && npm run dev",
    "check-duplicates": "node scripts/check-duplicates.js",
    "start:dev": "docker-compose up",
    "start:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "setup": "node scripts/setup.js"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "terser": "^5.26.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "description": "Modern logistics tracking and management solution",
  "main": "postcss.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timamikh/opti-track.git"
  },
  "keywords": [
    "logistics",
    "tracking",
    "management",
    "react",
    "keystone"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timamikh/opti-track/issues"
  },
  "homepage": "https://github.com/timamikh/opti-track#readme"
}
